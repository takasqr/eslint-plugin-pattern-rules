{
  "hosting": {
    "public": ".output/public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "trailingSlash": false,
    "redirects": [
      {
        "source": "/en",
        "destination": "https://eslint.regexp.app/en/",
        "type": 301
      },
      {
        "source": "/ja",
        "destination": "https://eslint.regexp.app/ja/",
        "type": 301
      }
    ]
  }
}
